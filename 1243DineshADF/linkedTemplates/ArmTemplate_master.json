{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "1243DineshADF"
		},
		"LS_ADLSGen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_ADLSGen2'"
		},
		"LS_BlbSA_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_BlbSA'"
		},
		"LS_FilesToSqlDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_FilesToSqlDB'"
		},
		"LS_Filesinsqltables_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_Filesinsqltables'"
		},
		"LS_github_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_github'"
		},
		"AzureBlobStoragekeyvault_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "Storage"
		},
		"LS_ADLSGen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sqltoadlsa.dfs.core.windows.net/"
		},
		"LS_Keyvault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://nareshkv1911.vault.azure.net/"
		},
		"LS_SQLKeyVault_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "SQLConnectionString"
		},
		"mykeyvault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://NareshKV1911.vault.azure.net/"
		},
		"Eventbasedtrigger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/65c1e3e2-3a44-4e95-8cb0-f0db3e99cee7/resourceGroups/1243Resourcegroup/providers/Microsoft.Storage/storageAccounts/1243dineshsa"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "1243DineshADF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_ADLSGen2_accountKey": {
						"value": "[parameters('LS_ADLSGen2_accountKey')]"
					},
					"LS_BlbSA_connectionString": {
						"value": "[parameters('LS_BlbSA_connectionString')]"
					},
					"LS_FilesToSqlDB_connectionString": {
						"value": "[parameters('LS_FilesToSqlDB_connectionString')]"
					},
					"LS_Filesinsqltables_connectionString": {
						"value": "[parameters('LS_Filesinsqltables_connectionString')]"
					},
					"LS_github_connectionString": {
						"value": "[parameters('LS_github_connectionString')]"
					},
					"LS_ADLSGen2_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLSGen2_properties_typeProperties_url')]"
					},
					"LS_Keyvault_properties_typeProperties_baseUrl": {
						"value": "[parameters('LS_Keyvault_properties_typeProperties_baseUrl')]"
					},
					"mykeyvault_properties_typeProperties_baseUrl": {
						"value": "[parameters('mykeyvault_properties_typeProperties_baseUrl')]"
					},
					"AzureBlobStoragekeyvault_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('AzureBlobStoragekeyvault_properties_typeProperties_connectionString_secretName')]"
					},
					"LS_SQLKeyVault_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LS_SQLKeyVault_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "1243DineshADF_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Eventbasedtrigger_properties_typeProperties_scope": {
						"value": "[parameters('Eventbasedtrigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', '1243DineshADF_ArmTemplate_0')]"
			]
		}
	]
}